<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://code.alibabatech.com/schema/dubbo http://mygame.xunlei.com/schema/dubbo/dubbo.xsd">

    <!-- <dubbo:annotation package="com.xunlei.youxi" /> -->

    <dubbo:application name="demo-consumer" owner="programmer" organization="dubbox" />
    <dubbo:registry address="zookeeper://127.0.0.1:2181" />
    <dubbo:consumer timeout="2000" retries="0"/>

    <dubbo:reference id="userService" interface="com.xunlei.youxi.dubbo.user.api.XLGameUserInfoService" />

    <bean class="com.xunlei.youxi.dubbo.user.consumer.Action" init-method="start">
        <property name="userService" ref="userService" />
    </bean>

</beans>
