<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://code.alibabatech.com/schema/dubbo http://mygame.xunlei.com/schema/dubbo/dubbo.xsd">

    <!-- <dubbo:annotation package="com.xunlei.youxi" /> -->
    <dubbo:application name="youxi-dubbo-user-provider" owner="youxi" organization="xunlei" />
    <dubbo:registry address="zookeeper://127.0.0.1:2181" />
    <dubbo:protocol name="dubbo" serialization="kryo" port="-1" />

    <bean id="userService" class="com.xunlei.youxi.dubbo.user.provider.XLGameUserInfoServiceImpl" />

    <dubbo:service ref="userService" protocol="dubbo" timeout="2000" retries="0" loadbalance="consistenthash" interface="com.xunlei.youxi.dubbo.user.api.XLGameUserInfoService" />

</beans>
